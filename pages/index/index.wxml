<!--index.wxml-->
<view>
  <view class="tabNav">
    <view wx:for="{{navTab}}" wx:key="index" data-name="{{item.type}}" bindtap="currentTab" class="{{currentTab==index ? 'cur' : ''}}"><text>{{item.name}}</text></view>
  </view>
  <view>
    <!-- {{item}} -->
    <list list="{{list}}"></list>
  </view>
</view>
